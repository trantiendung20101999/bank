<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ghost_white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageButton
        android:id="@+id/imbTaiKhoan"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/icon_bank_accounts"
        android:background="@drawable/imagebutton_background"></ImageButton>
    <TextView
        android:id="@+id/txtTaiKhoan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imbTaiKhoan"
        app:layout_constraintLeft_toLeftOf="@id/imbTaiKhoan"
        app:layout_constraintRight_toRightOf="@id/imbTaiKhoan"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Tài khoản"></TextView>

    <ImageButton
        android:id="@+id/imbThanhtoan"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintLeft_toRightOf="@id/imbTaiKhoan"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/bill"
        android:background="@drawable/imagebutton_background"></ImageButton>
    <TextView
        android:id="@+id/txtThanhtoan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imbThanhtoan"
        app:layout_constraintLeft_toLeftOf="@id/imbThanhtoan"
        app:layout_constraintRight_toRightOf="@id/imbThanhtoan"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Thanh toán"></TextView>

    <ImageButton
        android:id="@+id/imbNapDT"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:paddingLeft="15dp"
        android:paddingRight="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintLeft_toRightOf="@id/imbTaiKhoan"
        app:layout_constraintRight_toLeftOf="@id/imbThanhtoan"
        app:layout_constraintHorizontal_bias="0.5"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/cellphone"
        android:background="@drawable/imagebutton_background"></ImageButton>
    <TextView
        android:id="@+id/txtNapDT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imbNapDT"
        app:layout_constraintLeft_toLeftOf="@id/imbNapDT"
        app:layout_constraintRight_toRightOf="@id/imbNapDT"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Nạp Điện Thoại"></TextView>

    <ImageButton
        android:id="@+id/imbTietKiem"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:paddingLeft="15dp"
        android:paddingRight="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintTop_toBottomOf="@id/imbNapDT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintLeft_toRightOf="@id/imbTaiKhoan"
        app:layout_constraintRight_toLeftOf="@id/imbThanhtoan"
        app:layout_constraintHorizontal_bias="0.5"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/piggy_bank"
        android:background="@drawable/imagebutton_background"></ImageButton>
    <TextView
        android:id="@+id/txtTietKiem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imbTietKiem"
        app:layout_constraintLeft_toLeftOf="@id/imbTietKiem"
        app:layout_constraintRight_toRightOf="@id/imbTietKiem"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Tiết kiệm"></TextView>


    <ImageButton
        android:id="@+id/imbDauTu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@id/imbThanhtoan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintLeft_toRightOf="@id/imbTaiKhoan"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/investment"
        android:background="@drawable/imagebutton_background"></ImageButton>
    <TextView
        android:id="@+id/txtDauTu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imbDauTu"
        app:layout_constraintLeft_toLeftOf="@id/imbDauTu"
        app:layout_constraintRight_toRightOf="@id/imbDauTu"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Đầu tư"></TextView>

    <ImageButton
        android:id="@+id/imbGopY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@id/imbThanhtoan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/feedback"
        android:background="@drawable/imagebutton_background"></ImageButton>
    <TextView
        android:id="@+id/txtGopY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/imbGopY"
        app:layout_constraintLeft_toLeftOf="@id/imbGopY"
        app:layout_constraintRight_toRightOf="@id/imbGopY"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Góp Ý"></TextView>


</androidx.constraintlayout.widget.ConstraintLayout>